package deu.java002_02.server2;

import java.net.Socket;

public class CustomerThread extends ClientThread
{
	public CustomerThread(Socket _socket)
	{
		super(_socket);
	}

	@Override
	public void run()
	{
		while(super.getThreadState() == ThreadState.RUNNING)
		{
			System.out.println("Customer message: " + super.readLine());
			String header = super.readLine()
		}
	}
}
