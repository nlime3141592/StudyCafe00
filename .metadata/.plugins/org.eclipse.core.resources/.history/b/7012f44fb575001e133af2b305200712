package deu.java002_02.tprovider.gui.event;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import deu.java002_02.tprovider.gui.TestGui;
import deu.java002_02.tprovider.main.TProviderMain;
import deu.java002_02.tprovider.service.TService01;

public class ServiceEvent01 implements ActionListener
{
	private TestGui m_gui;
	
	public ServiceEvent01(TestGui _gui)
	{
		m_gui = _gui;
	}

	@Override
	public void actionPerformed(ActionEvent e)
	{
		new TService01().onService(TProviderMain.netModule);
	}
}
