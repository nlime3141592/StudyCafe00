package deu.java002_02.server2;

import java.io.UnsupportedEncodingException;
import java.net.Socket;

public class ProviderThread extends ClientThread
{
	public ProviderThread(Socket _socket)
	{
		super(_socket);
	}

	@Override
	public void run()
	{
		while(super.getThreadState() == ThreadState.RUNNING)
		{
			String line = super.readLine();
		}
	}
}
