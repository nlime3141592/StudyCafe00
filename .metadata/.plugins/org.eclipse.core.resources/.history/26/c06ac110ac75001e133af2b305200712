package deu.java002_02.server2;

import java.net.Socket;

import deu.java002_02.server2.service.IBusinessService;
import deu.java002_02.server2.service.INetworkService;
import deu.java002_02.server2.service.MyService01;

public class ProviderThread extends ClientThread
{
	public ProviderThread(Socket _socket)
	{
		super(_socket);
	}

	@Override
	protected void onService(String _header, INetworkService _netService)
	{
		IBusinessService service = null;

		switch(_header)
		{
		case "MY_SERVICE_01":
			service = new MyService01();
			break;
		default:
			System.out.println("잘못된 문자열 수신함.");
			break;
		}
	}
}
